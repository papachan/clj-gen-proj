{
  "name": "bun-project-cljs",
  "scripts": {
    "dev": "concurrently \"bun shadow:watch\" \"bun tailwind:watch\"",
    "prod": "bun shadow:prod && bun tailwind:release",
    "shadow:watch": "bunx shadow-cljs watch app",
    "shadow:prod": "bunx shadow-cljs release app;",
    "tailwind:watch": "bunx @tailwindcss/cli -i ./tailwind.css -o ./public/css/compiled.css --watch",
    "tailwind:release": "bunx @tailwindcss/cli -i ./tailwind.css -o ./public/css/compiled.css --minify"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.3",
    "concurrently": "^9.1.2",
    "react-refresh": "^0.14.0",
    "shadow-cljs": "^2.28.22",
    "tailwindcss": "^4.1.3"
  }
}
